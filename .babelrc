
{
    "presets": ["next/babel"],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "@components": "./components",
            "@utils": "./utils",
            "@pages": "./pages",
            "@styles": "./styles",
            "@services": "./services",
            "@constants": "./constants",
            "@containers": "./containers",
            "@redux": "./redux",
            "@hooks": "./hooks"

          }
        }
      ]
    ]
  }
  